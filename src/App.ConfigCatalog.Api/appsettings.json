{
      "AzureAd": {
        "Instance": "https://login.microsoftonline.com/",
        "TenantId": "common",
        "ClientId": "00000000-0000-0000-0000-000000000000",
        "Audience": "api://00000000-0000-0000-0000-000000000000"
      },
      "MultiTenant": {
        "AllowAnyTenant": false,
        "AllowedTenants": [
          "11111111-1111-1111-1111-111111111111"
        ]
      },
    "ConnectionStrings": {
        "ConfigCatalog": "Data Source=./App-config.db"
    },
    "ApplicationInsights": {
        "ConnectionString": ""
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "TokenHardening": {
        "MaxAccessTokenAgeMinutes": 15,
        "ClockSkewSeconds": 30,
        "EnableJtiReplayProtection": true,
        "JtiCacheMinutes": 20
    }
}
